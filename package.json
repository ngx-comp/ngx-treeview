    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.38",
    "codelyzer": "^6.0.2",
    "gh-pages": "^3.1.0",
    "jasmine-core": "^4.1.1",
    "jasmine-spec-reporter": "^7.0.0",
