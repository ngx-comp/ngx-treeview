  "description": "An Angular Boostrap treeview component with checkboxes",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:demo": "ng build angular-ngx-treeview --aot --output-hashing=all",
    "build": "ng-packagr -p ./projects/ngx-treeview/ng-package.json",
    "publish": "npm run build && npm publish --access public --folder ./dist/ngx-treeview",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run build:demo && node gh-pages.js"
  },
  "keywords": [
    "ng",
    "ngx",
    "angular",
    "ngx-treeview",
    "tree",
    "treeview",
    "tree-view",
    "checkbox",
    "dropdown",
    "recursive",
    "angular tree component"
  ],
  "author": "Charmedme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charmedme/ngx-treeview.git"
  },
  "bugs": {
    "url": "https://github.com/charmedme/ngx-treeview/issues"
  },
